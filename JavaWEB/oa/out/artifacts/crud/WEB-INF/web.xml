<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
<servlet>
    <servlet-name>user</servlet-name>
    <servlet-class>com.jpowernode.oa.web.action.User</servlet-class>
</servlet>
    <servlet-mapping>
        <servlet-name>user</servlet-name>
        <url-pattern>/user</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>updateServlet</servlet-name>
        <servlet-class>com.jpowernode.oa.web.action.updateServlet</servlet-class>
    </servlet>
<servlet-mapping>
    <servlet-name>updateServlet</servlet-name>
    <url-pattern>/updateServlet</url-pattern>
</servlet-mapping>


    <servlet>
        <servlet-name>Delete</servlet-name>
        <servlet-class>com.jpowernode.oa.web.action.Delete</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Delete</servlet-name>
        <url-pattern>/dept/detect</url-pattern>
    </servlet-mapping>



    <servlet>
        <servlet-name>DeptListServlet</servlet-name>
        <servlet-class>com.jpowernode.oa.web.action.DeptListServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>DeptListServlet</servlet-name>
        <url-pattern>/dept/list</url-pattern>
    </servlet-mapping>



    <servlet>
        <servlet-name>Details</servlet-name>
        <servlet-class>com.jpowernode.oa.web.action.Details</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Details</servlet-name>
        <url-pattern>/dept/detail</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>add</servlet-name>
        <servlet-class>com.jpowernode.oa.web.action.AddServlet</servlet-class>
    </servlet>
<servlet-mapping>
    <servlet-name>add</servlet-name>
    <url-pattern>/dept/add</url-pattern>
</servlet-mapping>


    <servlet>
        <servlet-name>adduser</servlet-name>
        <servlet-class>com.jpowernode.oa.web.action.AddUser</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>adduser</servlet-name>
        <url-pattern>/add/user</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>update</servlet-name>
        <servlet-class>com.jpowernode.oa.web.action.update</servlet-class>
    </servlet>
  <servlet-mapping>
      <servlet-name>update</servlet-name>
      <url-pattern>/update</url-pattern>
  </servlet-mapping>
</web-app>